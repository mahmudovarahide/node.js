<%- include('includes/head.ejs') %>

    <body>
        <%- include('includes/header.ejs') %>
        <main class="main-section">
            <% if(prods.length > 0) { %>
            <div class="d-flex">
                <% for(let product of prods){ %>
                <div class="box">
                    <h1><%= product.title %></h1>
                    <div class="image-box">
                        <img
                            src="https://www.clipartqueen.com/image-files/stable-colored-books.png"
                            alt="Books">
                    </div>
                    <div class="price-box">$19</div>
                </div>
                <% } %>
            </div>
            <% } else { %>
            <h1>not found</h1>
            <% } %>
        </main>
    </body>

</html>